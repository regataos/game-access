{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vue/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.getEnv"
  ],
  "modes": {
    "window": {
      "title": "game-access",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vue/public/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "game-access",
    "resourcesPath": "/vue/dist",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.6.0",
    "clientVersion": "5.6.0",
    "frontendLibrary": {
      "patchFile": "/vue/index.html",
      "devUrl": "http://localhost:5173/",
      "projectPath": "/vue/",
      "initCommand": "npm install",
      "devCommand": "BROWSER=none npm run dev",
      "buildComand": "npm run build"
    }
  }
}